{
  "name": "fito-theme",
  "displayName": "Fito Theme",
  "description": "Theme for VSCode with dark colors by RamiroMoralesdev",
  "publisher": "RamiroMoralesdev",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.93.0"
  },

  
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Fito Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/Fito Theme-color-theme.json"
      }
    ],
    "commands": [
      {
        "command": "fito-theme.enable",
        "title": "Enable Fito Theme"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "vscode:postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^1.60.0"
  }
}
